{
  "name": "idp-speech-to-text",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:dev": "ng build --configuration develop --output-hashing=none",
    "build:stg": "ng build --configuration stage",
    "concat:dev": "node concat-script.js idp-speech-to-text 070223 dev",
    "build:element:dev": "ng build --configuration development --output-hashing=none && node concat-script.js idp-speech-to-text 070223 dev",
    "build:element:stg": "ng build --configuration stage --output-hashing=none && node concat-script.js idp-speech-to-text 070223 stg",
    "build:element:prod": "ng build --configuration production --output-hashing=none && node concat-script.js idp-speech-to-text 070223"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.2.2",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/elements": "^15.2.3",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.2.2",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "concat": "^1.0.3",
    "fs-extra": "^11.1.1",
    "rxjs": "~7.5.0",
    "string-similarity": "^4.0.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.8.2"
  }
}
